@model IEnumerable<Twitch.Models.Canalstreamer>




@{
    ViewData["Title"] = "Index";
    List<Consulta2> listConsulta = (List<Consulta2>)ViewData["lista"];
}

<h1>Consulta 2</h1>

<p>
    contar quantos inscritos tem em cada canal
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                nome_streamer
            </th>
            <th>
                total_inscritos
            </th>


        </tr>
    </thead>
    <tbody>
        @foreach (Consulta2 con in listConsulta)
        {
            <tr>
                <td>
                    @con.Nome_streamer
                </td>
                <td>
                    @con.Total_inscritos
                </td>


            </tr>
        }
    </tbody>
</table>
