@model IEnumerable<Twitch.Models.Canalstreamer>




@{
    ViewData["Title"] = "Index";
    List<Consulta1> listConsulta = (List<Consulta1>) ViewData["lista"];
}

<h1>Consulta 1</h1>

<p>
    mostrar quem ainda não tem nenhuma recompensa reivindicada
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Streamer
            </th>
            <th>
               Itens Reinvindicados
            </th>
            
        
        </tr>
    </thead>
    <tbody>
        @foreach(Consulta1 con in listConsulta)
        {
            <tr>
                <td>
                    @con.Nome
                </td>
                <td>
                    @con.Itens_reivindicados
                </td>
               
                
            </tr>
        }
    </tbody>
</table>
